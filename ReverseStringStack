import java.util.*;
class ReverseStringStack
{
	public static void main(String[] ar)
	{
		Scanner sc= new Scanner(System.in);
		String s = sc.next();
		Stack<Character> st = new Stack<>();
		StringBuilder ab = new StringBuilder();
		for(char c : s.toCharArray())
			st.push(c);
		while(!st.isEmpty())
			ab.append(st.pop());
		System.out.println(ab);
	}
}
